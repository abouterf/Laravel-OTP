{
    "name": "abouterf/laravel-mobile-auth",
    "description": "laravel mobile authentication for laravel framework.",
    "keywords": [
        "abouterf",
        "laravel mobile auth",
        "mobile auth"
    ],
    "homepage": "https://github.com/",
    "license": "MIT",
    "authors": [{
        "name": "abouter",
        "email": "ekargosha88@gmail.com",
        "role": "developer"
    }],
    "require": {
        "php": ">=7.2",
        "illuminate/support": ">=6",
        "doctrine/dbal": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8"
    },
    "autoload": {
        "psr-4": {
            "Abouterf\\LaravelMobileAuth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Abouterf\\LaravelMobileAuth\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    }

}
